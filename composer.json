{
    "name": "rodosgrup/iyzi-laravel",
    "description": "Iyzico laravel package",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rodosgrup\\IyziLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rodosgrup\\IyziLaravel\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Rodos Yazılım",
            "email": "yazilim@rodosgrup.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1",
        "laravel/framework": "^9.2|^10.0",
        "iyzico/iyzipay-php": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RodosGrup\\IyziLaravel\\IyziLaravelProvider"
            ],
            "aliases": {
                "IyziLaravel": "RodosGrup\\IyziLaravel\\Facades\\IyziLaravel"
            }
        }
    }
}
